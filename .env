# Railway Gateway Environment Variables
PORT=3000
NODE_ENV=production

# Proxy Configuration
PRX_BANK_URL=https://raw.githubusercontent.com/backup-heavenly-demons/gateway/refs/heads/main/kvProxyList.json
REVERSE_PRX_TARGET=example.com

# Security (optional)
ENABLE_RATE_LIMITING=false
ALLOWED_ORIGINS=*

# UDP Configuration
UDP_TIMEOUT=30000
MAX_UDP_CONNECTIONS=100

# Logging
LOG_LEVEL=info
